[
  {
    "description":"模拟一个没有参数的get请求",
    "request":{
      "uri":"/demo",
      "method":"get"
    },

    "response":{
      "text":"这是一个没有参数的get请求"
    }
  },
  {
    "description":"模拟带参数的get请求",
    "request":{
      "uri":"/getwithparam",
      "method":"get",
      "queries":{
        "name":"chen",
        "age": "99"
      }
    }
    ,"response":{
      "text":"这是一个带参数的get请求"
    }
  }
]